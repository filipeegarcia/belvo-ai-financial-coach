{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/belvo/(.*)",
      "dest": "/frontend/$1"
    },
    {
      "src": "/belvo",
      "dest": "/frontend/"
    }
  ],
  "rewrites": [
    {
      "source": "/belvo/:path*",
      "destination": "/frontend/:path*"
    }
  ]
}
